name: Zori theme
type: theme
base theme: classy
description: 'Custom Responsive Drupal Theme of Zori'
package: Custom
core: 8.x
core_version_requirement: ^8 || ^9
regions:
  header: Header
  help: Help
  content: Content
  footer: Footer
  footer_bottom: Footer Bottom
  pop_up: Pop Up

libraries:
  - zori_theme/global-styling

libraries-override:
  classy/base:
    css:
      component:
        css/components/menu.css: false
        css/components/form.css: false
        css/components/inline-form.css: false
        css/components/button.css: false
